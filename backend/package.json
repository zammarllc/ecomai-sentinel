{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "description": "Backend core setup",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:ts": "tsx watch src/server.ts",
    "build": "tsc --project tsconfig.json",
    "start:ts": "node dist/server.js",
    "lint": "tsc --noEmit",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:seed": "node prisma/seed.js"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.54.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.18.0",
    "tsx": "^4.11.0",
    "typescript": "^5.3.3"
  }
}
